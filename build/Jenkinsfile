pipeline {
	agent any
    stages {
        stage ('lint') {
            steps {
			sh './build/stages/lint.sh'
            },
        stage ('build') {
            steps {
			sh './build/stages/build.sh'
            },      
        stage ('test') {
            steps {
			sh './build/stages/test.sh'
            },
        stage ('release') {
            steps {
			sh './build/stages/release.sh'
            } 
		}
    }
}